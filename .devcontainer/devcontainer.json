{
  "name": "internship-2024-5days",
  "dockerComposeFile": ["../docker-images/compose.yml", "compose.yml"],
  "service": "web",
  "workspaceFolder": "/app",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "tamasfe.even-better-toml",
        "mhutchie.git-graph",
        "GitHub.vscode-pull-request-github",
        "charliermarsh.ruff",
        "ms-azuretools.vscode-docker",
        "Glavin001.unibeautify-vscode",
        "batisteo.vscode-django"
      ]
    }
  },
  "containerEnv": {
    "FLASK_APP": "/app/src/web/app.py"
  }
}
